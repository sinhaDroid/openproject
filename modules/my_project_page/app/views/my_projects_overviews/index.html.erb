<% content_for :header_tags do %>
  <%= auto_discovery_link_tag(:atom, {:controller => 'activities',  :action => 'index', :id => @project, :format => 'atom', :key => User.current.rss_key}) %>
  <%= stylesheet_link_tag "my_project_page/my_projects_overview", :media => 'all' %>
<% end %>

<% html_title(t(:label_overview)) -%>

<%= toolbar title: t(:label_overview) %>

<div id="invisible-grid" class="widget-boxes project-overview">
  <% top_fields.each do |f| %>
    <%= rendered_field f %>
  <% end %>
  <div class="grid-block">
    <% middle_fields.each do |f| %>
      <%= rendered_field f %>
    <% end %>
  </div>
</div>
